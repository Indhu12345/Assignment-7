import java.util.*;
class FACTORIAL
{
public static void main(String args[])
{
Scanner scn=new Scanner(System.in);
System.out.println("enter the values");
int n=scn.nextInt();
int K=1;
if((n>0) && (n<=100)) //constrain
{
int a[]=new int[n];
for(int i=0;i<n;i++)
{
a[i]=scn.nextInt();
}
for(int i=0;i<n;i++)
{
for(int j=a[i];j>=1;j--)
{
K=K*j;
}
System.out.println(K);
K=1;
}  
}
}
}

OUTPUT:
enter the values
3
5
6
7
120
720
5040
